class Solution {
    public int[] searchRange(int[] nums, int target) {
        int first = find_first(nums,target);
        int last = find_last(nums,target);
        return new int[] {first,last};
    }
    public static int find_first(int [] nums,int target){
        int index = -1, start = 0, end = nums.length-1;
        while(start<=end){
            int mid = start + (end-start)/2;
            if(nums[mid] == target){
                index = mid;
                end = mid-1;
            }
            else if(nums[mid]<target){
                start = mid+1;
            }
            else end = mid-1;
        }
        return index;
    }
    public static int find_last(int [] nums,int target){
        int index = -1, start = 0, end = nums.length-1;
        while(start<=end){
            int mid = start + (end-start)/2;
            if(nums[mid] == target){
                index = mid;
                start = mid+1;
            }
            else if(nums[mid]<target){
                start = mid+1;
            }
            else end = mid-1;
        }
        return index;
    }
}
