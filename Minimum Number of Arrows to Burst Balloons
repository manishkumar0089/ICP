class Main{
    public static int findMinArrowShots(int[][] balloons){
        java.util.Arrays.sort(balloons,(a,b)->Integer.compare(a[1],b[1]));
        int arrows=1;
        int end=balloons[0][1];
        for(int i=1;i<balloons.length;i++){
            if(balloons[i][0]>end){
                arrows++;
                end=balloons[i][1];
            }
        }
        return arrows;
    }
}
